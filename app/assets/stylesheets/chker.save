div.cs-select {
    display: inline-block;
    position: relative;
    text-align: left;
    z-index: 1;
    -webkit-touch-callout: none;
    user-select: none;
    width: 100%;
    height: auto !important;
    box-shadow: none !important
}

select.cs-select,
.cs-select>label {
    display: none
}

div.cs-select:focus {
    outline: none;
    border-color: #ededed !important
}

.cs-select span {
    display: block;
    position: relative;
    cursor: pointer;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis
}

.cs-placeholder {
    top: 0px;
    font-size: 14px;
    font-size: 1.4rem;
    color: gray;
    text-transform: uppercase;
    letter-spacing: 2px;
    font-weight: 500;
    padding-top: 5px;
    padding: 16px 20px;
    background-color: #F5F9FB;
    border: 1px solid #D0E2EB;
    border-radius: 3px;
    margin-bottom: 0;
    line-height: 2
}

.form-control.cs-select {
    position: relative
}

.form-control.cs-select:after {
    content: '';
    position: absolute;
    right: 56px;
    top: 0;
    width: 1px;
    height: calc(100% - 2px);
    background: #D0E2EB
}

.cs-placeholder::after {
    content: '';
    background: url("../img/arrow_dropdown-blue.svg") 50% 50%/10.1px 5.8px no-repeat;
    width: 56px;
    height: 100px;
    right: 0;
    transform: translate3d(0%, -50%, 0);
    transition: all 0.3s
}

.cs-select>span::after,
.cs-select .cs-selected span::after {
    speak: none;
    position: absolute;
    top: 50%;
    transform: translateY(-50%)
}

.cs-select.cs-active>span::after,
.cs-select:hover>span::after {
    transform: translateY(-50%) rotate(180deg)
}

div.cs-active {
    z-index: 3
}

.cs-select.cs-active .cs-placeholder {
    border-radius: 3px 3px 0 0
}

.cs-select .cs-options {
    position: absolute;
    overflow-y: scroll;
    width: 100%;
    transform: translatey(-1px);
    margin-bottom: 2px;
    border-radius: 0 0 3px 3px;
    display: none;
    width: 100%;
    background-color: #F5F9FB;
    border: 1px solid #D0E2EB;
    max-height: 240px
}

.cs-select .cs-options:last-child {
    margin-bottom: 0
}

.cs-select.cs-active .cs-options {
    display: block
}

@media (max-width: 800px) {
    .cs-select.cs-active .cs-options {
        margin-bottom: 40px
    }
}

.cs-select .cs-options ul {
    list-style: none;
    margin: 0;
    padding: 0
}

.cs-select .cs-options ul li {
    background-position: 18px center;
    margin-bottom: 0
}

.cs-select .cs-options ul li:hover,
.cs-select .cs-options ul li.cs-focus span {
    background-color: #fff
}

.cs-select .cs-options ul span {
    padding: 15px 20px 13px
}

.cs-select li.cs-optgroup ul {
    padding-left: 1em
}

.cs-select li.cs-optgroup>span {
    cursor: default
}

div.cs-skin-slide {
    color: gray;
    font-size: 16px;
    font-size: 1.6rem
}

div.cs-skin-slide::before {
    content: '';
    background: white;
    position: absolute;
    width: 100%;
    height: 60px;
    top: 20px;
    left: 0;
    backface-visibility: hidden;
    transform-origin: 50% 0%;
    transition: transform 0.3s;
    border: 1px solid #bcbec0
}

.cs-skin-slide.cs-active::before {
    transform: scale3d(1, 3.5, 1);
    height: 71.5px;
    border: 0
}

.cs-skin-slide>span {
    height: 60px;
    line-height: 24px;
    transition: text-indent 0.3s, opacity 0.3s
}

@media screen and (max-width: 30em) {
    .cs-skin-slide>span {
        height: 60px;
        line-height: 28px
    }
}

.cs-skin-slide.cs-active>span {
    text-indent: -290px;
    opacity: 0
}

.cs-skin-slide.cs-active>span::after {
    transform: translate3d(0, -50%, 0)
}

.cs-skin-slide .cs-options {
    background: transparent;
    width: 100%;
    height: 60px;
    padding: 0;
    top: 29px;
    left: 50%;
    transition: transform 0.3s, height, 0.3s;
    transform: translate3d(-50%, -14.25%, 0);
    overflow: auto;
    border: 1px solid #bcbec0
}

.cs-skin-slide.cs-active .cs-options {
    height: 251px;
    top: 56px
}

.cs-skin-slide .cs-options li {
    opacity: 0;
    line-height: 35px;
    transform: translate3d(30%, 0, 0);
    transition: transform 0.3s, opacity 0.3s;
    border-bottom: 1px solid #dedfe0;
    margin-bottom: 0
}

.cs-skin-slide .cs-options li:before {
    display: none
